{
    "$schema": "https://raw.githubusercontent.com/wraith13/evil-type.ts/master/generated/schema/type.json#",
    "imports": [
        {
            "import": "{ EvilType }",
            "from": "./evil-type.ts/common/evil-type"
        }
    ],
    "defines": {
        "export": {
            "type": "code",
            "export": true,
            "tokens": [
                "{",
                "EvilType",
                "}"
            ]
        },
        "Type": {
            "type": "namespace",
            "members": {
                "SinglePrimeMode": {
                    "type": "interface",
                    "members": {
                        "base": {
                            "type": "string"
                        },
                        "template": {
                            "$ref": "Value"
                        },
                        "output": {
                            "$ref": "PathValue"
                        },
                        "parameters": {
                            "type": "or",
                            "types": [
                                {
                                    "type": "dictionary",
                                    "valueType": {
                                        "$ref": "Value"
                                    }
                                },
                                {
                                    "$ref": "JsonValue"
                                }
                            ]
                        }
                    },
                    "additionalProperties": false
                },
                "SingleMode": {
                    "type": "type",
                    "define": {
                        "type": "or",
                        "types": [
                            {
                                "$ref": "SinglePrimeMode"
                            },
                            {
                                "$ref": "BuildProcessTarget"
                            },
                            {
                                "$ref": "BuildReferenceTarget"
                            },
                            {
                                "$ref": "BuildMetaTarget"
                            }
                        ]
                    }
                },
                "Mode": {
                    "type": "type",
                    "define": {
                        "type": "or",
                        "types": [
                            {
                                "$ref": "SingleMode"
                            },
                            {
                                "$ref": "MultiMode"
                            }
                        ]
                    }
                },
                "Root": {
                    "type": "interface",
                    "members": {
                        "$schema": {
                            "const": "https://raw.githubusercontent.com/wraith13/build.js/master/json-schema.json#"
                        },
                        "modes": {
                            "type": "dictionary",
                            "valueType": {
                                "$ref": "Mode"
                            }
                        }
                    }
                }
            }
        }
    },
    "options": {
        "outputFile": "./type.ts",
        "indentUnit": 4,
        "indentStyle": "allman",
        "validatorOption": "full",
        "default": {
            "export": true,
            "additionalProperties": false
        },
        "schema": {
            "outputFile": "./generated/schema.json",
            "$id": "https://raw.githubusercontent.com/wraith13/build.js/master/json-schema.json#",
            "$ref": "Type.Root"
        }
    }
}